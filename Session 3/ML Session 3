y_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","source":["class regression:\n","  def __init__(self, nvar):\n","    self.intercept = 0\n","    self.w = np.random.randn(nvar)\n","\n","  def sigmoid(self, y):\n","    return 1/(1 + np.exp((-1)*y))\n","\n","  def update_w(self, X, y, y_pred, learning_rate):\n","    d_w = np.dot((y_pred - y), X)\n","    return self.w - (learning_rate * d_w)\n","\n","  def update_intercept(self, y, y_pred, learning_rate):\n","    d_intercept = np.sum(y_pred - y)\n","    return self.intercept - (learning_rate * d_intercept)\n","\n","  def cross_entropy(self, y, y_pred):\n","    ce = -1 * sum(y*np.log(y_pred) + (1-y)*np.log(1-y_pred))\n","    return ce\n","\n","  def fit(self, X, y, itr, learning_rate):\n","    loss = []\n","\n","    for i in range(itr):\n","      y_pred = self.predict(X, self.w, self.intercept)\n","      self.w = self.update_w(X, y, y_pred, learning_rate)\n","      self.intercept = self.update_intercept(y, y_pred, learning_rate)\n","      loss.append(self.cross_entropy(y, y_pred))\n","\n","    return loss\n","\n","  def predict(self, X, w, intercept):\n","    y_raw = np.array(intercept + (np.dot(X, w)))\n","    return self.sigmoid(y_raw)"],"metadata":{"id":"idkfIpzE01fJ","executionInfo":{"status":"ok","timestamp":1647599908361,"user_tz":-420,"elapsed":6,"user":{"displayName":"steven matthew","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GiUgsV2vJ0HGH1QdrN2FBPiolk3yFbf9DU31NS-3Q=s64","userId":"16378802003908441081"}}},"execution_count":317,"outputs":[]},{"cell_type":"code","source":["plt.plot(loss[0:], label=0.5)\n","plt.ylim([0, 0.1])\n","plt.xlabel(\"Iteration\")\n","plt.ylabel(\"Loss\")\n","plt.legend()\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":283},"id":"Vljp2fdLRUCO","executionInfo":{"status":"ok","timestamp":1647599908361,"user_tz":-420,"elapsed":6,"user":{"displayName"